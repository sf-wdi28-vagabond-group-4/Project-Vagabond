<%= form_for @city do |f| %>
  <%= f.text_field :city_name, placeholder: "City" %>
  <%= f.text_field :state, placeholder: "State" %>
  <%= f.text_field :country, placeholder: "Country" %>
  <%= f.text_field :city_picture, placeholder: "Picture Url" %>
  <%= f.text_field :header_picture, placeholder: "Header Url" %>
  <%= f.submit "Save City" %>
<% end %>
